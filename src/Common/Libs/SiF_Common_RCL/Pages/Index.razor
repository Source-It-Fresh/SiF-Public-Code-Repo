<style>
    h1 {
        color: #2b7920;
    }

    h4 {
        color: #2b7920;
    }
</style>

<MudPaper Class="d-flex align-center justify-center mud-width-full" Elevation="0">
    <MudGrid>
        <MudItem xs="12">
            <!-- Start Home Area -->
            <MudElement Class="home-area section-gap" id="home">
                <MudGrid>
                    <MudItem xs="12" md="6">
                        <MudText Class="p-5" Align="Align.Center" Typo="Typo.h1"><b>@_StringLocalizer["Title"]</b></MudText>
                        <MudText Class="m-2" Align="Align.Left" Typo="Typo.body1">
                            @_StringLocalizer["Greetings1"]<br />
                            @_StringLocalizer["Greetings2"]
                        </MudText>
                    </MudItem>
                    <MudItem xs="12" md="6">
                        <MudImage Class="img-fluid p-3 mt-4 mb-2" Src="_content/SiF_Common_RCL/Images/header1.png" Alt="Be part of beautiful change" />
                    </MudItem>
                </MudGrid>
            </MudElement>
            <!-- End Home Area -->
        </MudItem>

        <MudItem xs="12">
            <!-- Start About Area-->
            <MudElement Class="about-area section-gap" id="about">
                <MudGrid>
                    <MudItem xs="12" md="8">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudText Class="p-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Heading001"]</MudText>
                        <MudText Class="m-2" Align="Align.Left" Typo="Typo.body1">
                            @_StringLocalizer["Details001.1"]
                        </MudText>
                        <MudText Class="p-3" Align="Align.Left" Typo="Typo.subtitle2"><b>@_StringLocalizer["Details001.2"]</b></MudText>
                    </MudItem>
                    <MudItem xs="12" md="4">
                        <MudImage Class="img-fluid p-3 mt-4 mb-2" Src="_content/SiF_Common_RCL/Images/fruits.png" Alt="Produce something fabulous" />
                    </MudItem>
                </MudGrid>
            </MudElement>
            <!--End About Area-->
        </MudItem>

        <MudItem xs="12">
            <!-- Start Project Area-->
            <MudElement Class="project-area section-gap" id="project">
                <MudGrid>
                    <MudItem xs="12" md="4">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudCard>
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Class="p-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Producers"]</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudText Class="m-3" Align="Align.Left" Typo="Typo.body2">
                                    @_StringLocalizer["ProducersText"]
                                </MudText>
                            </MudCardContent>
                            <MudCardActions Class="d-flex justify-center">
                                <MudFab Href="/SiF_Common_RCL/Information/GrowersComponent" StartIcon="fas fa-hand-holding-water" Label=@_StringLocalizer["ViewDetails"] DropShadow="false" />
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                    <MudItem xs="12" md="4">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudCard>
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Class="m-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Suppliers"]</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudText Class="p-3" Align="Align.Left" Typo="Typo.body2">
                                    @_StringLocalizer["SuppliersText"]
                                </MudText>
                            </MudCardContent>
                            <MudCardActions Class="d-flex justify-center">
                                <MudFab Href="/SiF_Common_RCL/Information/SuppliersComponent" StartIcon="fas fa-parachute-box" Label=@_StringLocalizer["ViewDetails"] DropShadow="false" />
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                    <MudItem xs="12" md="4">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudCard>
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Class="m-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Consumers"]</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudText Class="p-3" Align="Align.Left" Typo="Typo.body2">
                                    @_StringLocalizer["ConsumersText"]
                                </MudText>
                            </MudCardContent>
                            <MudCardActions Class="d-flex justify-center">
                                <MudFab Href="/SiF_Common_RCL/Information/BuyersComponent" StartIcon="fas fa-utensils" Label=@_StringLocalizer["ViewDetails"] DropShadow="false" />
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                </MudGrid>

                <MudGrid Class="px-2">
                    <MudItem xs="12" md="4">
                        <MudImage Class="img-fluid p-3 mt-4 mb-2" Src="_content/SiF_Common_RCL/Images/3circle.png" Alt="Keep it turning" />
                    </MudItem>
                    <MudItem xs="12" md="8">
                        @*<MudElement Class="p-2 mt-5"></MudElement>*@
                        <MudText Class="p-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Heading002"]</MudText>
                        <MudText Class="m-2" Align="Align.Left" Typo="Typo.body1">
                            @_StringLocalizer["Details002.1"]<br />
                            @_StringLocalizer["Details002.2"]<br />
                            @_StringLocalizer["Details002.3"]<br />
                        </MudText>
                    </MudItem>
                </MudGrid>
            </MudElement>
            <!-- End Project Area -->
        </MudItem>

        <MudItem xs="12">
            <!-- Start DiveIn Area -->
            <MudElement Class="diveIn-area section-gap" id="diveIn">
                <MudGrid Class="px-2">
                    <MudItem xs="12" md="3">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudImage Class="img-fluid p-3 mb-2 rounded" Src="_content/SiF_Common_RCL/Images/foodprep.jpg" Alt="Make something fabulous" />
                    </MudItem>
                    <MudItem xs="12" md="6">
                        <MudPaper Class="p-3 mb-5 rounded">
                            <MudText Class="p-3" Align="Align.Center" Typo="Typo.h3">@_StringLocalizer["Heading003"]</MudText>
                            <MudText Class="m-2" Align="Align.Center" Typo="Typo.caption">
                                 @_StringLocalizer["Details003"]
                            </MudText>
                            <br />
                            <MudText Class="p-3" Align="Align.Center" Typo="Typo.caption">All from one FREE platform.</MudText>
                            <br />
                            <MudFab Href="#" Label="Take a test drive.." DisableElevation="false" />
                        </MudPaper>
                    </MudItem>
                    <MudItem xs="12" md="3">
                        @*<MudElement Class="mt-4"></MudElement>*@
                        <MudImage Class="img-fluid p-3 mb-2 rounded" Src="_content/SiF_Common_RCL/Images/catering.jpg" Alt="Make something fabulous" />
                    </MudItem>
                </MudGrid>

                <MudGrid Class="px-2">
                    <MudItem xs="12" md="6">
                        @*<MudElement Class="p-2 mt-3"></MudElement>*@
                        <MudCard>
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Class="p-3" Align="Align.Left" Typo="Typo.h4">@_StringLocalizer["Heading004"]</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudText Class="m-3" Align="Align.Left" Typo="Typo.body1">
                                    @_StringLocalizer["Details004.1"]<br />
                                    @_StringLocalizer["Details004.2"]<br />
                                    @_StringLocalizer["Details004.3"]<br />
                                    @_StringLocalizer["Details004.4"]<br />
                                </MudText>
                            </MudCardContent>
                            <MudCardActions>
                                <MudFab Href="/Core_Identity/Account/Login" Label="Producing already?" DisableElevation="false" />
                                <MudFab Href="/Core_Identity/Account/Register" Label="Want to start?" DisableElevation="false" />
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                    <MudItem xs="12" md="6">
                        <MudImage Class="img-fluid" Src="_content/SiF_Common_RCL/Images/food-basket.jpg" Alt="Grow something fabulous" />
                    </MudItem>
                </MudGrid>
            </MudElement>
            <!-- End DiveIn Area -->
        </MudItem>

        <MudItem xs="12">
            <!-- Start Price Area -->
            <MudElement Class="price-area section-gap" id="price">
