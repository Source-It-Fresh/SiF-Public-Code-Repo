                                <MudElement Class="d-flex justify-center">
                                    <MudText Typo="Typo.h3">FREE</MudText>
                                    <MudText Typo="Typo.h5" Class="ml-1 mt-5" Color="Color.Warning">/Monthly</MudText>
                                </MudElement>
                                <MudList T="string" Class="mx-auto mt-4" Style="width:300px;">
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Create trading entity</MudListItem>
