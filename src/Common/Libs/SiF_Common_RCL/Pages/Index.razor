                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                    <MudItem xs="12" sm="6" md="4">
                        <MudCard Elevation="25" Class="rounded-lg pb-4">
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Typo="Typo.h5" Align="Align.Center">Premium</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudElement Class="d-flex justify-center">
                                    <MudText Typo="Typo.h3">$15</MudText>
                                    <MudText Typo="Typo.h5" Class="ml-1 mt-5" Color="Color.Warning">/Monthly</MudText>
                                </MudElement>
                                <MudList T="string" Class="mx-auto mt-4" Style="width:300px;">
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Create trading entity</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Designate production areas</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Assign work spaces</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Allocate delegates</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Broadcast available produce</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Connect to community</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Access useful data</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Source geolocatable product</MudListItem>
                                </MudList>
                            </MudCardContent>
                            <MudCardActions Class="d-flex justify-center">
                                <MudButton Link="/Core_Identity/Account/Login" Variant="Variant.Filled" Color="Color.Primary" Size="Size.Large" Style="width:50%;">Select</MudButton>
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                    <MudItem xs="12" sm="12" md="4">
                        <MudCard Elevation="25" Class="rounded-lg pb-4">
                            <MudCardHeader>
                                <CardHeaderContent>
                                    <MudText Typo="Typo.h5" Align="Align.Center">Enterprise</MudText>
                                </CardHeaderContent>
                            </MudCardHeader>
                            <MudCardContent>
                                <MudElement Class="d-flex justify-center">
                                    <MudText Typo="Typo.h3">$99</MudText>
                                    <MudText Typo="Typo.h5" Class="ml-1 mt-5" Color="Color.Warning">/Monthly</MudText>
                                </MudElement>
                                <MudList T="string" Class="mx-auto mt-4" Style="width:300px;">
                                    <MudListItem Icon="@Icons.Material.TwoTone.DoneOutline" IconColor="Color.Success">All 'Premium' features</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Create educational entity</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Designate data spaces</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Assign roles</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Access global data - per species</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Broadcast value chain</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Allocate budget</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Collaborate with other institutions</MudListItem>
                                    <MudListItem Icon="@Icons.Material.TwoTone.Check">Seek out suppliers</MudListItem>
                                </MudList>
                            </MudCardContent>
                            <MudCardActions Class="d-flex justify-center">
                                <MudButton Link="/Core_Identity/Account/Login" Variant="Variant.Filled" Color="Color.Primary" Size="Size.Large" Style="width:50%;">Select</MudButton>
                            </MudCardActions>
                        </MudCard>
                    </MudItem>
                </MudGrid>
            </MudElement>
            <!-- End Price Area -->
        </MudItem>

